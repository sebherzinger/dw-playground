		section(id='contact' class='contact')
			div.container
				include modules/contact/intro.jade
				form(class='row' data-sr)
					div.col-xs-3.col-xs-offset-3
						div.row
							div.col-xs-12
								input(type='text' class='form-control' id='example01' placeholder='First Name')
